<h2>Exibitions</h2>

<div id="exibition-wrapper">
  <% @exibitions.each do |exibition| %>

  <div class="exibition">
    <h3><%= exibition.artist.alias %>: <a href="/visitors/<%= exibition.id %>"><%= exibition.title %></a></h3>
    <p>Category: <%= exibition.category %></p>
  </div>

  <% end %>
</div>
